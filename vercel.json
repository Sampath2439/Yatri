{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    }
  ],
  "outputDirectory": "dist/spa",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "functions": {
    "server/index.ts": {
      "maxDuration": 30
    }
  }
}
